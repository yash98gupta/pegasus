<body>
<div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
  <ol class="carousel-indicators">
    <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
    <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
    <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
  </ol>
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img class="d-block w-100 imgcr" src="/assets/pega-1.jpg" alt="First slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100 imgcr" src="/assets/pega-2.jpg" alt="Second slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100 imgcr" src="/assets/pega-3.JPG" alt="Third slide">
    </div>
  </div>
  <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>

<div class="inquiry_outer">
  <div class="inquiry_logo"><img src="/assets/pegasus-ogo.png"></div>
  <div class="inquiry_inner">
    <img src="/assets/message.png">
    <a href="" data-toggle="modal" data-target="#modalContactForm">Ask a Question </a>
  </div>
</div>





<div class="modal fade inquiry_form" id="modalContactForm" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content" style="background-color: #ff751a;">
          <%=form_for @inquiry, :url => {:controller => "homepage" , :action => "inquiry_create"} do |i| %>
            <div class="modal-header text-center">
                <h4 class="modal-title w-100 font-weight-bold">Write to us</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body mx-3">
                <div class="md-form mb-5">
                    <i class="fa fa-user prefix grey-text"></i>
                    <label data-error="wrong" data-success="right" for="form34">Your name</label><br>
                    <!-- <input type="text" id="form34" class="form-control validate"> -->
                    <%=i.text_field :name,style:"width:80%;"%>
                </div>

                <div class="md-form mb-5">
                    <i class="fa fa-envelope prefix grey-text"></i>
                    <label data-error="wrong" data-success="right" for="form29">Contact Number</label><br>
                    <!-- <input type="tel" id="form" class="form-control validate"> -->
                    <%=i.phone_field :mobile_number,style:"width:80%;"%>
                </div>

                <div class="md-form mb-5">
                    <i class="fa fa-tag prefix grey-text"></i>
                    <label data-error="wrong" data-success="right" for="form32">Subject</label><br>
                    <!-- <input type="text" id="form32" class="form-control validate"> -->
                    <%=i.text_field :subject,style:"width:80%;"%>
                </div>

                <div class="md-form">
                    <i class="fa fa-pencil prefix grey-text"></i>
                    <label data-error="wrong" data-success="right" for="form8">Your message</label><br>
                    <!-- <textarea type="text" id="form8" class="md-textarea form-control" rows="4"></textarea> -->
                    <%=i.text_area :message,style:"width:80%;"%>
                </div>

            </div>
            <div class="modal-footer d-flex justify-content-center">
                <!-- <button class="btn btn-unique" style="background-color: #ff6633;">Send <i class="fa fa-paper-plane-o ml-1"></i></button> -->
                <%=i.submit 'Submit', :class =>"btn btn-unique"%>
            </div>
            <%end%>
        </div>
    </div>
</div>



